{
  "name": "gemini",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Gemini is an open-source application powered by the Google Gemini Vision API. It enables users to identify and learn about objects captured by their camera through a simple and interactive experience. Just say 'Hey Gemini' and show an object to the camera and say!",
  "keywords": [
    "Gemini",
    "Google Gemini",
    "Gemini Vision",
    "Image Recognition",
    "Multimodal AI",
    "Open Source",
    "Computer Vision"
  ],
  "author": "Srikanth Nani",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/youkpan/gemini-assistant.git"
  },
  "bugs": {
    "url": "https://github.com/youkpan/gemini-assistant/issues"
  },
  "homepage": "https://v.stylee.top:3000/",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/generative-ai": "^0.11.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^2.0.3",
    "@vercel/analytics": "^1.1.1",
    "alibabacloud-nls": "^1.0.4",
    "annyang": "^2.6.1",
    "microsoft-cognitiveservices-speech-sdk": "^1.36.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-speech-recognition": "^3.10.0"
  },
  "devDependencies": {
    "@types/annyang": "^2.6.5",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-speech-recognition": "^3.9.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
